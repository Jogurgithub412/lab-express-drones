<h2>Available drones</h2>

{{#each drones}}
  <div>
    <p>name:{{name}}</p>
    <p>propeller:{{propeller}}</p>
    <p>Max Speed:{{maxSpeed}}</p>
     <a href="/drones/{{_id}}/edit">Edit</a>
    <form action="/drones/{{_id}}/delete" method="post">
      <button>Delete</button>
    </form>
  </div>
{{/each}}